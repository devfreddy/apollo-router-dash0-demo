              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "query": "avg by (host,pod_name,container_id) ({otel_metric_name=\"apollo_router_cache_size\", otel_metric_type=\"histogram\"})"
