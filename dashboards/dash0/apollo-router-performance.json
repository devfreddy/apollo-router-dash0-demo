          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "query": "sum by (subgraph.name) ({otel_metric_name=\"http_client_request_duration\", otel_metric_type=\"histogram\", graphql.errors=\"true\"})"
